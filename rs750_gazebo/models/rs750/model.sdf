<sdf version='1.6'>
  <model name='rs750'>
    <pose>0 0 0 0 0 0</pose>
    <static>false</static>
    <enable_wind>true</enable_wind>

    <plugin name="link_joint_controller" filename="libLinkJointControllerPlugin.so">
      <input_topic>/cmd_vel</input_topic>
      <link_name>thruster_link</link_name>
      <link_scale>5</link_scale>
      <link_type>velocity</link_type>
      <link_pid>1000 0 10</link_pid>
      <joint_name>rudder_joint</joint_name>
      <joint_scale>0.8</joint_scale>
      <joint_type>position</joint_type>
      <joint_pid>1000 0 10</joint_pid>
    </plugin>

    <plugin name="link_joint_controller2" filename="libLinkJointControllerPlugin.so">
      <input_topic>/cmd_vel2</input_topic>
      <link_name>__default__</link_name>
      <link_scale>1</link_scale>
      <link_type>velocity</link_type>
      <link_pid>1000 0 10</link_pid>
      <joint_name>main_sail_joint</joint_name>
      <joint_scale>0.8</joint_scale>
      <joint_type>position</joint_type>
      <joint_pid>1000 0 10</joint_pid>
    </plugin>

    <plugin name="link_joint_controller3" filename="libLinkJointControllerPlugin.so">
      <input_topic>/cmd_vel2</input_topic>
      <link_name>__default__</link_name>
      <link_scale>1</link_scale>
      <link_type>velocity</link_type>
      <link_pid>1000 0 10</link_pid>
      <joint_name>jib_sail_joint</joint_name>
      <joint_scale>0.8</joint_scale>
      <joint_type>position</joint_type>
      <joint_pid>1000 0 10</joint_pid>
    </plugin>

    <plugin name="hydrodynamics" filename="libHydrodynamicsPlugin.so">
      <!-- Wave Model -->
      <wave_model>ocean_waves</wave_model>

      <!-- Hydrodynamics -->
      <damping_on>true</damping_on>
      <viscous_drag_on>true</viscous_drag_on>
      <pressure_drag_on>true</pressure_drag_on>

      <!-- Linear and Angular Damping -->
      <cDampL1>1.0E-6</cDampL1>
      <cDampL2>1.0E-6</cDampL2>
      <cDampR1>1.0E-6</cDampR1>
      <cDampR2>1.0E-6</cDampR2>

      <!-- 'Pressure' Drag -->
      <cPDrag1>1.0E+2</cPDrag1>
      <cPDrag2>1.0E+2</cPDrag2>
      <fPDrag>0.4</fPDrag>
      <cSDrag1>1.0E+2</cSDrag1>
      <cSDrag2>1.0E+2</cSDrag2>
      <fSDrag>0.4</fSDrag>
      <vRDrag>1.0</vRDrag>

      <!-- Markers -->
      <markers>
        <update_rate>30</update_rate>
        <water_patch>false</water_patch>
        <waterline>false</waterline>
        <underwater_surface>false</underwater_surface>
      </markers>
    </plugin>

    <plugin name="main_sail_liftdrag" filename="libLiftDragPlugin.so">
      <a0>0.08727</a0>
      <alpha_stall>0.3368</alpha_stall>
      <cla>5.418</cla>
      <cla_stall>-2.1419</cla_stall>
      <cda>0.0</cda>
      <cda_stall>0.0</cda_stall>
      <area>1.81</area>
      <fluid_density>1.225</fluid_density>
      <forwards>1 0 0</forwards>
      <upward>0 1 0</upward>
      <cp>0.46 0 1.96</cp>
      <link_name>main_sail_link</link_name>
      <radial_symmetry>true</radial_symmetry>
    </plugin>

    <!-- Hull -->
    <link name="base_link">
      <visual name="hull_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://rs750/meshes/rs750_hull.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Red</name>
          </script>
        </material>
      </visual>

      <visual name="deck_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://rs750/meshes/rs750_deck.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Gray</name>
          </script>
        </material>
      </visual>

      <visual name="keel_fin_visual">
        <pose>-0.16255 -0.00512 -0.54053 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://rs750/meshes/rs750_keel_fin.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Gray</name>
          </script>
        </material>
      </visual>

      <visual name="keel_bulb_visual">
        <pose>-0.26638 0.01038 -1.08674 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://rs750/meshes/rs750_keel_bulb.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Gray</name>
          </script>
        </material>
      </visual>

      <!-- Rigging  -->
      <visual name="mast_visual">
        <pose>0 0 2.09 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://rs750/meshes/rs750_mast.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Gray</name>
          </script>
        </material>
      </visual>

      <collision name="base_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://rs750/meshes/rs750_hull_collision_3.stl</uri>
          </mesh>
        </geometry>
      </collision>

      <collision name="keel_fin_collision">
       <pose>-0.16255 -0.00512 -0.54053 0 0 0</pose>
         <geometry>
          <mesh>
            <uri>model://rs750/meshes/rs750_keel_fin_collision_2.stl</uri>
          </mesh>
        </geometry>
      </collision>

      <inertial>
        <pose>-0.1 0 -0.75 0 0 0</pose>
        <mass>60</mass>
        <inertia>
          <ixx>10</ixx>
          <ixy>0</ixy>
          <iyy>50</iyy>
          <iyz>0</iyz>
          <izz>43</izz>
        </inertia>
      </inertial>

      <!-- Sensors -->
      <sensor name="mast_head_anemometer" type="anemometer">
        <pose>0 0 4.049375 0 0 0</pose>
        <always_on>true</always_on>
        <update_rate>50</update_rate>
        <topic>mast_head_anemometer</topic>
      </sensor>
      <visual name="mast_head_anemometer_visual">
        <pose>0 0 4.049375 0 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.025</radius>
            <length>0.025</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Red</name>
          </script>
        </material>
      </visual>

      <sensor name="mast_base_anemometer" type="anemometer">
        <pose>0 0 0.130625 0 0 0</pose>
        <always_on>true</always_on>
        <update_rate>50</update_rate>
        <topic>mast_head_anemometer</topic>
      </sensor>
      <visual name="mast_base_anemometer_visual">
        <pose>0 0 0.130625 0 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.025</radius>
            <length>0.025</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Red</name>
          </script>
        </material>
      </visual>

    </link>

    <!-- Rudder -->
    <link name="rudder_link">
      <pose>-1.2436 -0.00323 -0.0487 0 0 0</pose>

      <visual name="rudder_visual">
        <geometry>
          <mesh>
            <uri>model://rs750/meshes/rs750_rudder.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Gray</name>
          </script>
        </material>
      </visual>

      <collision name="rudder_collision">
        <geometry>
          <mesh>
            <uri>model://rs750/meshes/rs750_rudder_collision_3.stl</uri>
          </mesh>
        </geometry>
      </collision>

      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.1</ixx>
          <ixy>0</ixy>
          <iyy>0.1</iyy>
          <iyz>0</iyz>
          <izz>0.1</izz>
        </inertia>
      </inertial>
    </link>

    <joint type="revolute" name="rudder_joint">
      <pose>0 0 0 0 0 0</pose>
      <child>rudder_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <initial_position>0</initial_position>
        <limit>
          <lower>-0.8</lower>
          <upper>0.8</upper>
        </limit>
      </axis>
    </joint>

    <!-- Main Sail -->
    <link name="main_sail_link">
      <pose>-0.02625 0 0.291875 0 0 0</pose>

      <visual name="main_sail_visual">
        <geometry>
          <mesh>
            <uri>model://rs750/meshes/rs750_main_sail.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Gray</name>
          </script>
        </material>
      </visual>

      <visual name="main_boom_visual">
        <pose>-0.463125 0 -0.01125 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://rs750/meshes/rs750_main_boom.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Gray</name>
          </script>
        </material>
      </visual>

      <collision name="main_sail_collision">
        <geometry>
          <mesh>
            <uri>model://rs750/meshes/rs750_main_sail.stl</uri>
          </mesh>
        </geometry>
      </collision>

      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.1</ixx>
          <ixy>0</ixy>
          <iyy>0.1</iyy>
          <iyz>0</iyz>
          <izz>0.1</izz>
        </inertia>
      </inertial>

      <!-- Sensors -->
      <sensor name="main_sail_anemometer" type="anemometer">
        <pose>-0.30875 0 1.30625 0 0 0</pose>
        <always_on>true</always_on>
        <update_rate>50</update_rate>
        <topic>main_sail_anemometer</topic>
      </sensor>
      <visual name="main_sail_anemometer_visual">
        <pose>-0.30875 0 1.30625 0 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.025</radius>
            <length>0.025</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Red</name>
          </script>
        </material>
      </visual>

    </link>

    <joint type="revolute" name="main_sail_joint">
      <pose>0 0 0 0 0 0</pose>
      <child>main_sail_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <initial_position>0</initial_position>
        <limit>
          <lower>-0.8</lower>
          <upper>0.8</upper>
        </limit>
      </axis>
    </joint>

    <!-- Jib Sail -->
    <link name="jib_sail_link">
      <pose>1.03125 0 0.235625 0 0 0</pose>

      <visual name="jib_sail_visual">
        <geometry>
          <mesh>
            <uri>model://rs750/meshes/rs750_jib_sail.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Gray</name>
          </script>
        </material>
      </visual>

      <visual name="jib_boom_visual">
        <pose>-0.463125 0 -0.01125 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://rs750/meshes/rs750_jib_boom.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Gray</name>
          </script>
        </material>
      </visual>

      <collision name="jib_sail_collision">
        <geometry>
          <mesh>
            <uri>model://rs750/meshes/rs750_jib_sail.stl</uri>
          </mesh>
        </geometry>
      </collision>

      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.1</ixx>
          <ixy>0</ixy>
          <iyy>0.1</iyy>
          <iyz>0</iyz>
          <izz>0.1</izz>
        </inertia>
      </inertial>

      <!-- Sensors -->
      <sensor name="jib_sail_anemometer" type="anemometer">
        <pose>-0.605666667 0 1.20528 0 0 0</pose>
        <always_on>true</always_on>
        <update_rate>50</update_rate>
        <topic>jib_sail_anemometer</topic>
      </sensor>
      <visual name="jib_sail_anemometer_visual">
        <pose>-0.605666667 0 1.20528 0 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.025</radius>
            <length>0.025</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Red</name>
          </script>
        </material>
      </visual>

    </link>

    <joint type="revolute" name="jib_sail_joint">
      <pose>0 0 0 0 0 0</pose>
      <child>jib_sail_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>-0.271652766 0 0.962395332</xyz>
        <initial_position>0</initial_position>
        <limit>
          <lower>-0.8</lower>
          <upper>0.8</upper>
        </limit>
      </axis>
    </joint>

    <!-- Thruster -->
    <link name="thruster_link">
      <pose>0 0 0.5 0 0 0</pose>
      <visual name="thruster_visual">
        <geometry>
          <box>
            <size>0.05 0.05 0.05</size>
          </box>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Black</name>
          </script>
        </material>
      </visual>
      <collision name="thruster_collision">
        <geometry>
          <box>
            <size>0.05 0.05 0.05</size>
          </box>
        </geometry>
      </collision>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.1</ixx>
          <ixy>0</ixy>
          <iyy>0.1</iyy>
          <iyz>0</iyz>
          <izz>0.1</izz>
        </inertia>
      </inertial>
    </link>

    <joint type="revolute" name="thruster_joint">
      <pose>0 0 0 0 0 0</pose>
      <child>thruster_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <initial_position>0</initial_position>
        <limit>
          <lower>-0.01</lower>
          <upper>0.01</upper>
        </limit>
      </axis>
    </joint>

  </model>
</sdf>
